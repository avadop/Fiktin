on () {\r\n      userCollection.doc(this.userKey).delete()\r\n      store.commit('logOut')\r\n      this.$router.push('/')\r\n    },\r\n    switchEdit: function () {\r\n      this.edit = !this.edit\r\n    },\r\n    newName (value) {\r\n      this.name = value\r\n    },\r\n    newNick (value) {\r\n      this.nick = value\r\n    },\r\n    newEmail (value) {\r\n      this.email = value\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.btn {\r\n    margin: 10px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}