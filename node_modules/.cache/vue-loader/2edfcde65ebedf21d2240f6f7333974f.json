k=\"nick\" :name=\"name\" @updateData=\"modifications\"/> -->\r\n      <button class=\"btn\" @click=\"switchEdit\"> Cancelar</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { userCollection } from '../firebase.js'\r\nimport ModifyUser from '@/components/ModifyUser.vue'\r\nimport { store } from '../store/index.js'\r\n\r\nexport default {\r\n  name: 'User',\r\n  components: {\r\n    ModifyUser\r\n  },\r\n  data () {\r\n    return {\r\n      nick: '',\r\n      email: '',\r\n      name: '',\r\n      password: '',\r\n      edit: false\r\n    }\r\n  },\r\n  props: {\r\n    userKey: {\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  created () {\r\n    userCollection.doc(this.userKey).get().then(doc => {\r\n      const data = doc.data()\r\n      this.nick = data.nick\r\n      this.email = data.email\r\n      this.name = data.name\r\n      this.password = data.password\r\n    })\r\n  },\r\n  methods: {\r\n    deleteUser: function () {\r\n      userCollection.doc(this.userKey).delete()\r\n      store.commit('logOut')\r\n      this.$router.push('/')\r\n    },\r\n    switchEdit: function () {\r\n      this.edit = !this.edit\r\n    },\r\n    newName (value) {\r\n      this.name = value\r\n    },\r\n    newNick (value) {\r\n      this.nick = value\r\n    },\r\n    newEmail (value) {\r\n      this.email = value\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.btn {\r\n    margin: 10px;\r\n}\r\n</style>\r\n"]}]}